{
	"framework": "mocha+chai",
	"src_files": [
		"development/spec/spec.js"
	],
	"launchers": {
		"node": {
			"command": "./node_modules/.bin/mocha -r spec/config/setup.js -R tap dist/spec/*.js",
			"protocol": "tap"
		}
	},
	"launch_in_dev": [
		"phantomjs",
		"chrome"
	],
	"launch_in_ci": [
		"phantomjs"
	]
}